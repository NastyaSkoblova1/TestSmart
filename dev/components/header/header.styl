.header
	
	&__row
		display flex
		justify-content space-between
		align-items center
	
		&._third
			padding 30pe 11%
			border-bottom 1px solid #dedede
	
	nav
		ul
			@extend $list-reset
			display flex
		li
			&:not(:last-child)
				padding-right 30pe
		a
			@extend $link-reset
			font-size 16pe
